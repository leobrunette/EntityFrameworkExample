<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>
	@Styles.Render("~/Content/css")
	@Styles.Render("https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap4.min.css")
	@Scripts.Render("~/bundles/modernizr")
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js")
	@Scripts.Render("https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap4.min.js")
    @Scripts.Render("~/Scripts/Site.js")
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Barrels Co.</p>
        </footer>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
